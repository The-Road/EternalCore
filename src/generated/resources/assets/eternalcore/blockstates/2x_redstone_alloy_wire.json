{
  "multipart": [
    {
      "apply": {
        "model": "eternalcore:block/wire_core_3"
      }
    },
    {
      "when": {
        "up": "on"
      },
      "apply": {
        "model": "eternalcore:block/wire_side_3"
      }
    },
    {
      "when": {
        "down": "on"
      },
      "apply": {
        "model": "eternalcore:block/wire_side_3",
        "x": 180
      }
    },
    {
      "when": {
        "south": "on"
      },
      "apply": {
        "model": "eternalcore:block/wire_side_3",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "north": "on"
      },
      "apply": {
        "model": "eternalcore:block/wire_side_3",
        "x": 90
      }
    },
    {
      "when": {
        "west": "on"
      },
      "apply": {
        "model": "eternalcore:block/wire_side_3",
        "x": 90,
        "y": 270
      }
    },
    {
      "when": {
        "east": "on"
      },
      "apply": {
        "model": "eternalcore:block/wire_side_3",
        "x": 90,
        "y": 90
      }
    }
  ]
}